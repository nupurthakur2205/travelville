pipeline{
  agent any
    stages{
      stage("build"){
        steps{
          echo 'build the application'
         
        }
      }
       stage("test"){
        steps{
          echo 'test the application'
          echo 'test 2'
        }
      }
       stage("deploy"){
        steps{
          echo 'deploy the application'
        }
      }
    }
}
      
      
